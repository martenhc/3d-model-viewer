{
  "name": "lit-scaffold",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "lit-localize build && vite build",
    "preview": "vite preview  --port 8080 --open",
    "deploy": "npm run build && gcloud app deploy app.yaml --project ${npm_package_name} --promote --quiet",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts}'",
    "lint:lit-analyzer": "lit-analyzer --quiet",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "lit": "^2.2.6",
    "three": "^0.140.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/node": "^14.17.34",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@web/rollup-plugin-html": "^1.10.1",
    "eslint": "^8.2.0",
    "eslint-plugin-lit-a11y": "^1.1.0-next.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "^3.1.0",
    "lit-analyzer": "^2.0.0-pre.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rollup": "^2.60.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-summary": "^1.3.0",
    "ts-lit-plugin": "^1.0.5",
    "typescript": "^4.5.2",
    "vite": "^2.6.4",
    "vite-plugin-favicon2": "^1.1.4",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-imagemin": "^0.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7",
    "yarn": "unsupported"
  }
}
